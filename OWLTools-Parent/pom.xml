	</dependencyManagement>
	<distributionManagement>
		<repository>
			<id>BBOPDeployRepository</id>
			<name>BBOPDeployRepository</name>
			<url>${bbopdeployrepository}</url>
		</repository>
		<snapshotRepository>
			<id>BBOPSnapshotRepository</id>
			<name>BBOPSnapshotRepository</name>
			<url>${bbopsnapshotrepository}</url>
		</snapshotRepository>
	</distributionManagement>

	<modules>
		<module>../OWLTools-Core</module>
		<module>../OWLTools-Annotation</module>
		<module>../OWLTools-Oort</module>
		<module>../OWLTools-Runner</module>
		<module>../OWLTools-Solr</module>
		<module>../OWLTools-Web</module>
		<module>../OWLTools-NCBI</module>
		<module>../OWLTools-Sim</module>
		<module>../Lego</module>
		<module>../Golr-Client</module>
	</modules>
</project>
